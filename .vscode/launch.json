    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "enter program name, for example ${workspaceFolder}/a.out",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "C/C++(RISCV) - Debug OpenSBI  Payload",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}",
            "args": [],
            "stopAtEntry": true,
            "targetArchitecture": "arm",
            "cwd": "${workspaceFolder}",
            "environment": [],
            "miDebuggerPath": "/home/user/baili/toolchain/bin/riscv64-unknown-elf-gdb", // RISC-V 工具链中的 gdb
            "miDebuggerServerAddress": "0.0.0.0:1234",
            "externalConsole": true,
            // "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    // "text": "-enable-pretty-printing",
                    "text": "set target-async off",
                    "ignoreFailures": false,
                    // "target remote": 1234
                }
            ],
            "launchCompleteCommand": "None",
            "customLaunchSetupCommands": [
                {
                    "text": "file /home/user/learn_abc/opensbi/build/platform/generic/firmware/fw_payload.elf",
                    "description": "load file to gdb",
                    "ignoreFailures": false
                },
                {
                    "text": "target remote :1234",
                    "description": "connect to server",
                    "ignoreFailures": false
                },
                {
                    "text": "b _start",
                    "description": "set breakpoints at _start",
                    "ignoreFailures": false
                },
            ],
        },
